{
  "name": "baptisteries.exploration",
  "version": "0.0.1",
  "description": "tool for exploration of early christian baptisteries",
  "main": "index.js",
  "repository": "git@github.com:adammertel/baptisteries.exploration.git",
  "author": "adammertel <mertel.adam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean-build": "rm -f -r -d build",
    "start": "parcel app/index.html --open --out-dir build",
    "build": "yarn run clean-build && parcel build app/index.html --out-dir build",
    "deploy": "rsync -avz build/ hde@hde.geogr.muni.cz:/var/www/virthtml/hde/baptisteries-exploration",
    "build-deploy": "npm run build && npm run deploy"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "mobx-react-devtools": "^6.0.3",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bulma": "^0.7.1",
    "chroma-js": "^1.3.7",
    "font-awesome": "^4.7.0",
    "global": "^4.3.2",
    "konva": "^2.3.0",
    "leaflet": "^1.3.4",
    "leaflet.markercluster": "^1.4.0",
    "leaflet.markercluster.placementstrategies": "^0.1.6",
    "mobx": "^4.3.1 || ^5.0.0",
    "mobx-react": "^4.0.0 || ^5.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-konva": "^1.7.14",
    "react-leaflet": "^2.0.1",
    "react-leaflet-markercluster": "^2.0.0-rc3",
    "standard": "^12.0.1"
  }
}
